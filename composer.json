{
    "name": "vaelar/nova",
    "description": "Nova API Project",
    "version": "0.1.4",
    "type": "project",
    "require": {
      "vaelar/nova-api": "^0.1.3",
      "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
      "psr-4": {
        "nova\\": "src/"
      }
    },
    "config": {
      "sort-packages": true,
      "optimize-autoloader": true
    },
    "scripts": {
      "post-root-package-install": [
        "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
      ]
    }
  }
  